{
  "baseUrl": "./solarishost.github.io/",
  "locals": {
    "url": "http://solarishost.github.io",
    "name": "SolarisHost Bot Control Panel",
    "owner": "Kornphatthanan",
    "description": "This is the control panel for the discord bot"
  },
  "plugins": [
    "./miniorange-login-openid/"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3,
    "groupSort": {
      "Quick Start": 1000,
      "Customization": 900,
      "Components": 800,
      "Other": 100
    }
  }
}
